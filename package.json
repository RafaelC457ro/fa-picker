{
  "name": "fa-picker",
  "version": "0.0.6",
  "description": "Create a Fonts Awesome Picker ",
  "main": "dist/fa-picker.js",
  "scripts": {
    "build": "node builder.js && rollup -c",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "npm run prettify && xo --fix",
    "prettify": "prettier --single-quote --no-semi --no-bracket-spacing --trailing-comma none --write \"src/**/*.js\" --write rollup.config.js --write builder.js"
  },
  "author": "Rafael Castro",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-latest": "^6.24.1",
    "cssnano": "^3.10.0",
    "font-awesome": "^4.7.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-modules": "^0.8.0",
    "prettier": "^1.3.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-json": "^2.1.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.5.0",
    "xo": "^0.18.2"
  },
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      [
        "latest",
        {
          "es2015": {
            "modules": false
          }
        }
      ]
    ],
    "plugins": [
      "external-helpers"
    ]
  },
  "xo": {
    "semicolon": false,
    "space": true,
    "envs": [
      "browser"
    ],
    "rules": {
      "import/extensions": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RafaelC457ro/fa-picker.git"
  },
  "keywords": [
    "font",
    "awesome",
    "picker",
    "font-awesome"
  ],
  "bugs": {
    "url": "https://github.com/RafaelC457ro/fa-picker/issues"
  },
  "homepage": "https://github.com/RafaelC457ro/fa-picker#readme"
}
